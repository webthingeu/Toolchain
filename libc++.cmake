set(CMAKE_BUILD_TYPE "Release" CACHE STRING "")
set(CMAKE_INSTALL_MESSAGE "NEVER" CACHE STRING "")
set(CMAKE_INSTALL_PREFIX "/System" CACHE STRING "")
set(CMAKE_MESSAGE_LOG_LEVEL "WARNING" CACHE STRING "")
set(CMAKE_WARN_DEPRECATED OFF CACHE BOOL "")

set(LLVM_DEFAULT_TARGET_TRIPLE "${WEBTHING_TARGET_TRIPLE}" CACHE STRING "")
set(LLVM_ENABLE_LLD ON CACHE BOOL "")
set(LLVM_ENABLE_PER_TARGET_RUNTIME_DIR OFF CACHE BOOL "")
set(LLVM_ENABLE_RUNTIMES "libcxxabi;libcxx;libunwind" CACHE STRING "")
set(LLVM_PARALLEL_LINK_JOBS "2" CACHE STRING "")

set(LIBCXX_ENABLE_ABI_LINKER_SCRIPT OFF CACHE BOOL "")
set(LIBCXX_ENABLE_STATIC OFF CACHE BOOL "")
set(LIBCXX_ENABLE_STATIC_ABI_LIBRARY ON CACHE BOOL "")
set(LIBCXX_HARDENING_MODE "extensive" CACHE STRING "")
set(LIBCXX_HAS_ATOMIC_LIB OFF CACHE BOOL "")
set(LIBCXX_INCLUDE_BENCHMARKS OFF CACHE BOOL "")
set(LIBCXX_INCLUDE_DOCS OFF CACHE BOOL "")
set(LIBCXX_INCLUDE_TESTS OFF CACHE BOOL "")
set(LIBCXX_INSTALL_STATIC_LIBRARY OFF CACHE BOOL "")
set(LIBCXX_STATICALLY_LINK_ABI_IN_SHARED_LIBRARY ON CACHE BOOL "")
set(LIBCXX_USE_COMPILER_RT ON CACHE BOOL "")
set(LIBCXXABI_ENABLE_SHARED OFF CACHE BOOL "")
set(LIBCXXABI_ENABLE_STATIC_UNWINDER ON CACHE BOOL "")
set(LIBCXXABI_INSTALL_STATIC_LIBRARY OFF CACHE BOOL "")
set(LIBCXXABI_STATICALLY_LINK_UNWINDER_IN_SHARED_LIBRARY ON CACHE BOOL "")
set(LIBCXXABI_USE_LLVM_UNWINDER ON CACHE BOOL "")

set(LIBUNWIND_USE_COMPILER_RT ON CACHE BOOL "")
set(LIBUNWIND_ENABLE_SHARED OFF CACHE BOOL "")
set(LIBUNWIND_INSTALL_STATIC_LIBRARY OFF CACHE BOOL "")
