set(CMAKE_BUILD_TYPE "Release" CACHE STRING "")
set(CMAKE_INSTALL_MESSAGE "NEVER" CACHE STRING "")
set(CMAKE_INSTALL_PREFIX "/System" CACHE STRING "")
set(CMAKE_MESSAGE_LOG_LEVEL "WARNING" CACHE STRING "")
set(CMAKE_WARN_DEPRECATED OFF CACHE BOOL "")

set(LLVM_BUILD_LLVM_DYLIB ON CACHE BOOL "")
set(LLVM_DEFAULT_TARGET_TRIPLE "${WEBTHING_TARGET_TRIPLE}" CACHE STRING "")
set(LLVM_DYLIB_COMPONENTS "all" CACHE STRING "")
set(LLVM_ENABLE_LIBCXX ON CACHE BOOL "")
set(LLVM_ENABLE_LLD ON CACHE BOOL "")
set(LLVM_ENABLE_PER_TARGET_RUNTIME_DIR ON CACHE BOOL "")
set(LLVM_ENABLE_PROJECTS "clang;lld" CACHE STRING "")
set(LLVM_ENABLE_RUNTIMES "compiler-rt" CACHE STRING "")
set(LLVM_FORCE_VC_REVISION "${WEBTHING_BUILD_REVISION}" CACHE STRING "")
set(LLVM_FORCE_VC_REPOSITORY "WebThing" CACHE STRING "")
set(LLVM_INCLUDE_DOCS OFF CACHE BOOL "")
set(LLVM_INCLUDE_EXAMPLES OFF CACHE BOOL "")
set(LLVM_INCLUDE_TESTS OFF CACHE BOOL "")
set(LLVM_INCLUDE_BENCHMARKS OFF CACHE BOOL "")
set(LLVM_INSTALL_BINUTILS_SYMLINKS ON CACHE BOOL "")
set(LLVM_INSTALL_TOOLCHAIN_ONLY ON CACHE BOOL "")
set(LLVM_LINK_LLVM_DYLIB ON CACHE BOOL "")
set(LLVM_PARALLEL_LINK_JOBS "2" CACHE STRING "")
set(LLVM_TARGETS_TO_BUILD Native CACHE BOOL "")

set(CLANG_BUILD_EXAMPLES OFF CACHE BOOL "")
set(CLANG_CONFIG_FILE_SYSTEM_DIR "../etc" CACHE STRING "")
set(CLANG_DEFAULT_CXX_STDLIB "libc++" CACHE STRING "")
set(CLANG_DEFAULT_LINKER "lld" CACHE STRING "")
set(CLANG_DEFAULT_RTLIB "compiler-rt" CACHE STRING "")
set(CLANG_INCLUDE_TESTS OFF CACHE BOOL "")
set(CLANG_LINK_CLANG_DYLIB ON CACHE BOOL "")

set(COMPILER_RT_BUILD_LIBFUZZER OFF CACHE BOOL "")
set(COMPILER_RT_BUILD_MEMPROF OFF CACHE BOOL "")
set(COMPILER_RT_BUILD_ORC OFF CACHE BOOL "")
set(COMPILER_RT_BUILD_PROFILE OFF CACHE BOOL "")
set(COMPILER_RT_BUILD_SANITIZERS OFF CACHE BOOL "")
set(COMPILER_RT_BUILD_XRAY OFF CACHE BOOL "")
set(COMPILER_RT_DEFAULT_TARGET_TRIPLE "${WEBTHING_TARGET_TRIPLE}" CACHE STRING "")
set(COMPILER_RT_INCLUDE_TESTS OFF CACHE BOOL "")
